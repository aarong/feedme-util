{
  "name": "feedme-util",
  "version": "0.3.4",
  "description": "Various Javascript utilities related to the Feedme specification.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "pretest": "npm run lint",
    "test": "jest",
    "coverage": "jest --coverage",
    "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "docs": "documentation build src/index.js -f html -o docs --access public --access private --access undefined --sort-order alpha",
    "prebuild": "npm run test",
    "build": "babel-node build.js"
  },
  "dependencies": {
    "check-types": "^11.2.3",
    "crypto-js": "^3.1.9-1",
    "json-expressible": "^1.0.1",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.8",
    "@babel/register": "^7.0.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "coveralls": "^3.0.3",
    "documentation": "^9.1.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^5.1.3",
    "ghooks": "^2.0.4",
    "glob": "^10.3.10",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "prettier": "^3.2.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!<rootDir>/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aarong/feedme-util.git"
  },
  "author": "Aaron G",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aarong/feedme-util/issues"
  },
  "homepage": "https://github.com/aarong/feedme-util#readme",
  "config": {
    "ghooks": {
      "pre-commit": "npm run build"
    }
  }
}
