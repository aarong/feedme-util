{
  "name": "feedme-util",
  "version": "0.2.3",
  "description": "Various Javascript utilities related to the Feedme specification.",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/**",
    "pretest": "npm run lint",
    "test": "./node_modules/.bin/jest",
    "coverage": "./node_modules/.bin/jest --coverage",
    "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "docs": "./node_modules/.bin/documentation build src/index.js -f html -o docs --access public --access private --access undefined --sort-order alpha",
    "prebuild": "npm run test",
    "build": "./node_modules/.bin/gulp build"
  },
  "dependencies": {
    "ajv": "^8.6.2",
    "check-types": "^7.4.0",
    "crypto-js": "^3.1.9-1",
    "debug": "^4.1.1",
    "json-expressible": "^1.0.1",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "coveralls": "^3.0.3",
    "del": "^4.0.0",
    "documentation": "^9.1.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "ghooks": "^2.0.4",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "prettier": "^1.14.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!<rootDir>/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aarong/feedme-util.git"
  },
  "author": "Aaron G",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aarong/feedme-util/issues"
  },
  "homepage": "https://github.com/aarong/feedme-util#readme",
  "config": {
    "ghooks": {
      "pre-commit": "npm run build"
    }
  }
}
